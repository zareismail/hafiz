{
    "name": "zareismail/hafiz",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/many-to-many": "~0.1",
        "zareismail/nova-contracts": "~0.1",
        "zareismail/quick-theme": "dev-master",
        "zareismail/nova-profile": "dev-master",
        "zareismail/shaghool": "~0.1",
        "zareismail/complex": "~0.1",
        "zareismail/details": "~0.1",
        "zareismail/costable": "~0.1",
        "zareismail/bonchaq": "~0.1",
        "zareismail/chapar": "~0.1",
        "zareismail/maintainable": "~1.0",
        "zareismail/nova-location": "~0.1",
        "zareismail/nova-cascade": "~0.2",
        "superlatif/nova-tag-input": "^1.4",
        "dillingham/nova-items-field": "^0.6.0", 
        "genealabs/nova-map-marker-field": "^0.3.0",
        "staudenmeir/belongs-to-through": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Hafiz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Hafiz\\HafizServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
