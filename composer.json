{
    "name": "zareismail/hafiz",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/many-to-many": "*",
        "zareismail/nova-contracts": "*",
        "staudenmeir/belongs-to-through": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Hafiz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Hafiz\\HafizServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
